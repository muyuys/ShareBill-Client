.search_input {
  height: 90rpx;
  padding: 5px 10px;
  background-color: var(--themeColor3);
  display: flex;
  align-items: center;
  border-radius: 55rpx;
}
.search_input .filed {
  border-radius: 45rpx;
  width: 100%;
  height: 80%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: var(--inputFieldColor);
  color: var(--textUnselectedColor);
}
